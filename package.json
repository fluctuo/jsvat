{
  "name": "jsvat-demo",
  "version": "2.5.2",
  "scripts": {
    "start": "ng serve --hmr",
    "build:prod": "ng build --prod --aot --output-hashing=all --build-optimizer --outputPath static && yarn move:index && yarn replace",
    "move:index": "cpx ./static/index.html ./ && rimraf ./static/index.html",
    "replace": "yarn replace:src && yarn replace:href",
    "replace:src": "./node_modules/.bin/replace-in-file /src=\\\"/g src=\\\"jsvat/static/ ./index.html --isRegex true",
    "replace:href": "./node_modules/.bin/replace-in-file /href=\\\"styles/g href=\\\"jsvat/static/styles ./index.html --isRegex true",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.4",
    "@angular/cdk": "7.2.0",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "7.1.4",
    "@angular/material": "7.2.0",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/router": "7.1.4",
    "core-js": "2.5.7",
    "jsvat": "2.5.2",
    "prismjs": "1.16.0",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.0",
    "@angular-devkit/build-optimizer": "0.13.7",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/language-service": "7.1.4",
    "@types/node": "8.9.4",
    "@types/prismjs": "1.16.0",
    "codelyzer": "4.4.3",
    "cpx": "1.5.0",
    "install": "0.12.2",
    "node-sass": "4.13.1",
    "replace-in-file": "4.1.0",
    "rimraf": "2.6.3",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-immutable": "4.7.0",
    "typescript": "3.1.6"
  }
}
